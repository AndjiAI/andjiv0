{
  "name": "@andji/scripts",
  "version": "1.0.0",
  "description": "Scripts package for Andji",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test",
    "debug-log-subset": "bun run debug-log-subset.ts",
    "typecheck-all": "bun run typecheck-all.ts"
  },
  "sideEffects": false,
  "engines": {
    "bun": ">=1.2.11"
  },
  "dependencies": {
    "@andji/bigquery": "workspace:*",
    "@andji/backend": "workspace:*",
    "@andji/common": "workspace:*",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/bun": "^1.2.11",
    "@types/lodash": "^4.14.195",
    "@types/node": "22"
  }
}
